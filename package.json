{
  "name": "ascent",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "lint": "eslint . --fix",
    "types:check": "astro check && tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.2",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "@calcom/embed-react": "^1.3.0",
    "@fontsource-variable/inter": "^5.0.8",
    "@fontsource-variable/montserrat": "^5.0.9",
    "@fontsource-variable/roboto-mono": "^5.0.9",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@uidotdev/usehooks": "^2.3.1",
    "astro": "^3.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cn": "^0.1.1",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swiper": "^10.3.1",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.0",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "tailwindcss-animated": "^1.0.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{astro,js,ts,tsx}": [
      "bash -c 'npm run types:check'",
      "eslint --fix",
      "prettier -w --plugin=prettier-plugin-astro"
    ],
    "*.{md,json}": [
      "prettier -w --plugin=prettier-plugin-astro"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
